<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.madhav.loginandreg.resultActivity">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="107dp"
        android:layout_height="63dp"
        android:text="Information Test"
        android:textColor="@color/colorPrimary"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

  <LinearLayout
      android:layout_width="0dp"
      android:layout_height="495dp"
      tools:layout_editor_absoluteY="8dp"
      tools:layout_editor_absoluteX="8dp"></LinearLayout>
    <TextView
        android:id="@+id/textView7"
        android:layout_width="73dp"
        android:layout_height="65dp"
        android:background="@android:color/holo_orange_dark"
        android:hint="Test1 Score"
        android:textSize="25dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView6"
        android:layout_marginLeft="33dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="74dp"
        android:layout_height="63dp"
        android:background="@android:color/holo_green_dark"
        android:hint="IQ Level"
        android:textSize="25dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView7"
        android:layout_marginLeft="33dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="107dp"
        android:layout_height="63dp"
        android:text="Comprehen Test"
        android:textColor="@color/colorPrimary"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="73dp"
        android:layout_height="65dp"
        android:background="@android:color/holo_orange_dark"
        android:hint="Test2 Score"
        android:textSize="25dp"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        android:layout_marginLeft="33dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="74dp"
        android:layout_height="63dp"
        android:background="@android:color/holo_green_dark"
        android:hint="IQ Level"
        android:textSize="25dp"
        android:layout_marginTop="37dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        android:layout_marginLeft="33dp" />

  <Button
      android:id="@+id/button3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="BackToMain"
      android:background="@android:color/holo_blue_dark"
      android:layout_marginTop="140dp"
      app:layout_constraintTop_toBottomOf="@+id/textView10"
      android:layout_marginLeft="141dp"
      app:layout_constraintLeft_toLeftOf="parent"
      android:onClick="back"/>

</android.support.constraint.ConstraintLayout>
